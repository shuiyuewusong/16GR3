<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_account" schema="dbo" catalog="db_user2"
		class="com.qhit.lh.gr3.cj.t4.bean.Account">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
	<table name="t_user" schema="dbo" catalog="db_user2"
		class="com.qhit.lh.gr3.cj.t4.bean.User">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="t_work" schema="dbo" catalog="db_user2"
	class="com.qhit.lh.gr3.cj.t4.bean.Work">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>